{
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#252525",
					"name": "Foreground",
					"slug": "foreground"
				},
				{
					"color": "#ffffff",
					"name": "Background",
					"slug": "background"
				},
				{
					"color": "#9f393d",
					"name": "Link",
					"slug": "link"
				},
				{
					"color": "#bf4c51",
					"name": "Link Hover",
					"slug": "link-hover"
				}
			]
		},
		"layout": {
			"contentSize": "620px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "'Iowan Old Style', 'Palatino Linotype', 'URW Palladio L', P052, serif",
					"name": "Oldstyle Serif",
					"slug": "system-oldstyle"
				},
				{
					"fontFamily": "Superclarendon, 'Bookman Old Style', 'URW Bookman', 'URW Bookman L', 'Georgia Pro', Georgia, serif",
					"name": "Antique Serif",
					"slug": "system-antique"
				},
				{
					"fontFamily": "Didot, 'Bodoni MT', 'Noto Serif Display', 'URW Palladio L', P052, Sylfaen, serif",
					"name": "Modern Serif",
					"slug": "system-modern"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Latin Modern Roman",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/latin-modern-roman_normal_400.woff2"
							]
						},
						{
							"fontFamily": "Latin Modern Roman",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/latin-modern-roman_normal_700.woff2"
							]
						},
						{
							"fontFamily": "Latin Modern Roman",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/latin-modern-roman_italic_700.woff2"
							]
						},
						{
							"fontFamily": "Latin Modern Roman",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/latin-modern-roman_italic_400.woff2"
							]
						}
					],
					"fontFamily": "'Latin Modern Roman', Cambria, 'Times New Roman', Times, serif",
					"slug": "latin-modern-roman"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/heading": {
				"css": "h2{margin-top:3rem}h3,h6{margin-top:2.5rem}h4{margin-top:2rem}h5{margin-top:1.8rem}h1+h2{margin-top:1.625rem}h2+h3,h3+h4,h4+h5{margin-top:.8rem}h5+h6{margin-top:-.8rem}h2,h3,h4,h5,h6{margin-bottom:.8rem}"
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"h2": {
				"typography": {
					"fontSize": "1.7rem",
					"lineHeight": "2"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "1.4rem",
					"lineHeight": "1.6"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "1.2rem",
					"lineHeight": "1.6"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "1rem",
					"lineHeight": "1.6"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "1rem",
					"fontStyle": "italic",
					"fontWeight": "400",
					"lineHeight": "1.6"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--link-hover)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--link)"
				}
			}
		},
		"spacing": {
			"blockGap": "1rem",
			"padding": {
				"bottom": "var(--wp--preset--spacing--50)",
				"left": "var(--wp--preset--spacing--50)",
				"right": "var(--wp--preset--spacing--50)",
				"top": "var(--wp--preset--spacing--50)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--latin-modern-roman)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json"
}